@model eProjectP3.Models.medical_type

@{
    ViewBag.Title = "Medicine Type";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPage.cshtml";
}

<div class="wrapper-table">
    <h3>Are you sure you want to delete this?</h3>
    <dl class="dl-horizontal">
        <dt>
            Medicine Type Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Medicine_Type_Name)
        </dd>
    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="wrapper-btn">
            <span class="btn"><span><input type="submit" value="Delete"/></span></span>
            <a href="/admin/medicinetypes/index" class="btn"><span>Back to List</span></a>
        </div>
    }
</div>
